{
  "name": "myconsulting-network",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "deploy": "gcloud run deploy myconsulting-network --source . --region=us-central1 --allow-unauthenticated",
    "deploy:logs": "gcloud run services logs read myconsulting-network --region=us-central1 --tail",
    "deploy:url": "gcloud run services describe myconsulting-network --region=us-central1 --format='value(status.url)'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@react-three/drei": "^9.117.3",
    "@react-three/fiber": "^8.17.10",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "next": "^16.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "three": "^0.170.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/three": "^0.170.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.0.7",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
